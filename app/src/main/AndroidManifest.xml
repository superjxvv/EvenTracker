<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.testingv1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProfileActivity" />
        <activity
            android:name=".LoginActivity" />
        <activity
            android:name=".addEvent" />
        <activity
            android:name=".EventsToday"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".EditEvent" />
        <activity
            android:name=".EmailFeedBack"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".GroupList"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".groupCalendar"
            android:parentActivityName=".GroupList" />
        <activity
            android:name=".addGroup"
            android:parentActivityName=".GroupList" />
        <activity
            android:name=".FindUsers"
            android:parentActivityName=".FriendList" />
        <activity
            android:name=".UserInfo"
            android:parentActivityName=".FindUsers" />
        <activity
            android:name=".RequestList"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".EventsTodayGroup"
            android:parentActivityName=".groupCalendar" />
        <activity
            android:name=".groupDetails" />
        <activity
            android:name=".MemberViewHolder" />
        <activity
            android:name=".FriendList"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".setName"
            android:parentActivityName=".addEventGroup"
            android:windowSoftInputMode="adjustNothing"/>
        <activity
            android:name=".checkMemberViewHolder" />
        <activity
            android:name=".Notifications"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".addEventGroup"></activity>
    </application>

</manifest>